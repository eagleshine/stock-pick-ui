{{#paper-content class='md-whiteframe-z1 grid-list'}}
  {{#paper-grid-list
    cols="1 md-2 gt-md-6"
    rowHeight="gt-md-1:1 2:2"
    gutter="8px gt-sm-8px" as |grid|
  }}
    {{#each model as |ticker|}}
      {{#grid.tile class="gray" rowspan="1" colspan="sm-1 gt-sm-2" as |tile|}}
		<div>
          {{ticker.name}}
 		</div>
        {{#tile.footer}}
          {{ticker.symbol}}
		{{/tile.footer}}
      {{/grid.tile}}
    {{/each}}
  {{/paper-grid-list}}
{{/paper-content}}
